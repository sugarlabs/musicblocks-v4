{
    "extends": "../tsconfig.json",
    "include": [
        "./**/*.ts",
        "./**/*.tsx",
        "./.storybook/**/*.ts",
        "./.storybook/**/*.tsx"
    ],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "#/@types/*": [
                "../@types/*"
            ],
            "@res/*": [
                "../res/*"
            ]
        }
    }
}
