{
  "name": "musicblocks",
  "version": "4.1.0",
  "description": "A complete overhaul of Music Blocks",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sugarlabs/musicblocks-v4.git"
  },
  "homepage": "https://sugarlabs.github.io/musicblocks-v4",
  "keywords": [
    "musicblocks"
  ],
  "author": "Anindya Kundu <anindya.k22@outlook.com> (https://github.com/meganindya/)",
  "license": "AGPL-3.0",
  "module": "true",
  "scripts": {
    "start": "webpack serve -c webpack/webpack.config.ts --env env=dev",
    "prebuild": "npx rimraf build",
    "build:root": "webpack -c webpack/webpack.config.ts --env env=prod --json=./build/stats.json",
    "build": "cross-env BASE_PATH='/musicblocks-v4/' webpack -c webpack/webpack.config.ts --env env=prod --json=./build/stats.json",
    "lint": "eslint src && textlint --rule terminology --rule no-dead-link README.md",
    "serve": "npx http-server build/ -p 5000 -g",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "preanalyze": "npm run build",
    "analyze": "webpack-bundle-analyzer --port 8000 build/stats.json",
    "test:unit": "jest",
    "test:e2e": "cypress run"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/jquery": "^3.5.14",
    "@types/jqueryui": "^1.12.16",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.11.41",
    "@types/p5": "^1.4.2",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.27",
    "@typescript-eslint/parser": "5.27",
    "babel-loader": "^8.2.5",
    "buffer": "^6.0.3",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cypress": "^10.6.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsonc": "^2.5.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.3",
    "jqueryui": "^1.11.1",
    "jsonc-loader": "^0.1.1",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "2.3.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "textlint": "^12.2.1",
    "textlint-rule-no-dead-link": "^4.8.0",
    "textlint-rule-no-empty-section": "^1.1.0",
    "textlint-rule-terminology": "^2.1.6",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.8.1",
    "typescript": "~4.7.4",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "dependencies": {
    "@sugarlabs/musicblocks-v4-lib": "^0.2.0",
    "jquery": "^3.6.0",
    "js-yaml": "^3.14.1",
    "p5": "^1.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.6",
    "tone": "^14.7.77",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
