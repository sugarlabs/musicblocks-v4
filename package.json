    "@storybook/testing-library": "^0.0.13",
    "@sugarlabs/mb4-assets": "*",
    "@sugarlabs/mb4-config": "*",
    "@sugarlabs/mb4-events": "*",
    "@sugarlabs/mb4-i18n": "*",
    "@testing-library/jest-dom": "^5.16.4",
