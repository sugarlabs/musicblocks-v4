{
  "name": "@sugarlabs/musicblocks4",
  "version": "4.2.0",
  "description": "A complete overhaul of Music Blocks",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sugarlabs/musicblocks-v4.git"
  },
  "homepage": "https://sugarlabs.github.io/musicblocks-v4",
  "keywords": [
    "musicblocks"
  ],
  "author": "Anindya Kundu <anindya.k22@outlook.com> (https://github.com/meganindya/)",
  "license": "AGPL-3.0",
  "module": "true",
  "scripts": {
    "serve": "vite -c tools/vite.config.ts --host --port 5173",
    "stats": "ts-node tools/scripts/stats",
    "build": "vite build -c tools/vite.config.ts --base / && npm run stats",
    "build:gh": "vite build -c tools/vite.config.ts --base /musicblocks-v4/ && npm run stats",
    "preview": "vite preview -c tools/vite.config.ts --host --port 4173 --base /",
    "preview:gh": "vite preview -c tools/vite.config.ts --host --port 4173 --base /musicblocks-v4/",
    "visualize": "npm run preview -- --open http://localhost:4173/stats.html",
    "predeploy": "npm run dist",
    "deploy": "gh-pages -d dist",
    "check": "tsc",
    "lint": "eslint src && markdownlint -i src/components/editor-next src && textlint --rule terminology --rule no-dead-link README.md",
    "test:unit": "jest",
    "test:e2e": "cypress run",
    "storybook": "storybook dev -p 6006",
    "storybook-build": "storybook build -o storybook",
    "storybook-serve": "http-server storybook -p 6007"
  },
  "devDependencies": {
