    "jsonc-parser": "^3.2.0",
    "lerna": "^6.6.2",
    "markdownlint": "^0.27.0",
    "markdownlint-cli": "^0.33.0",
    "open": "^8.4.0",
    "prettier": "2.3.1",
    "prop-types": "^15.8.1",
    "rollup-plugin-visualizer": "5.9.0",
